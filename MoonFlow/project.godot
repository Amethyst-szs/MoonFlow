; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MoonFlow"
config/description="A Super Mario Odyssey project management & mod development tool"
run/main_scene="res://scene/main/main.tscn"
run/print_header=false
config/project_settings_override="user://common.cfg"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=10000
boot_splash/bg_color=Color(0.054902, 0.0745098, 0.105882, 1)
boot_splash/image="res://icon.png"
boot_splash/fullsize=false
config/icon="res://iconS.png"

[autoload]

DisplayServerUtility="*res://scene/settings/engine/global/display_server_utility.gd"
AutoloadInit="*res://addons/AutoloadInit.cs"
EngineSettings="*res://scene/settings/engine/global/engine_setting_holder.gd"
ScreenshotCapture="*res://scene/common/screenshot/screenshot_capture.gd"

[dotnet]

project/assembly_name="MoonFlow"
project/solution_directory="res://../"

[editor]

run/main_run_args="-- --launchmode=default"

[editor_plugins]

enabled=PackedStringArray("res://addons/SmoothScroll/plugin.cfg", "res://addons/git_database/plugin.cfg", "res://addons/markdownlabel/plugin.cfg", "res://addons/mfgraph_resource/plugin.cfg", "res://addons/podb/plugin.cfg", "res://addons/wiki/plugin.cfg")

[gui]

timers/tooltip_delay_sec=0.07

[importer_defaults]

nindot.msbt={
"tag_library": 1
}

